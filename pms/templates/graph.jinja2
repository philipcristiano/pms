<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
	<title>Rollup</title>
	<link href="/static/css/basic.css" type="text/css" rel="stylesheet" />
	<script type="text/javascript" src="/static/shared/jquery.min.js"></script>
	<script type="text/javascript" src="/static/js/jquery.flot.js"></script>
</head>
<body>

    <div id="placeholder" style="width:800px;height:200px;"></div>

<script type="text/javascript">
$(function () {
    var options = {};
    var options2 = {
        xaxis: {
            mode: "time",
        }
    };
    var dummy_data = [];
    var d1 = [[0, 3], [4, 8], [8, 5], [9, 13]];
    var placeholder = $("#placeholder");
    var plot = $.plot(placeholder, [d1], options2);
    var t;
    function update() {
        $.getJSON('/rollups/2011/8/1/host-level', {}, function(data){
            plot.setData(data['response']);
            plot.setupGrid();
            plot.draw();
            t = setTimeout(update, 1000);
        });
    }
    t = setTimeout(update, 1000);
    });

</script>

</body>
</html>
