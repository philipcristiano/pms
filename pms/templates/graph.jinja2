<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
	<title>Rollup</title>
	<link href="/static/css/basic.css" type="text/css" rel="stylesheet" />
	<script type="text/javascript" src="/static/shared/jquery.min.js"></script>
	<script type="text/javascript" src="/static/js/jquery.flot.js"></script>
	<script type="text/javascript" src="/static/shared/EnhanceJS/enhance.js"></script>
	<script type="text/javascript">
		// Run capabilities test
		enhance({
			loadScripts: [
				{src: '/static/js/excanvas.js', iecondition: 'all'},
				'/static/js/visualize.jQuery.js',
				'/static/js/example.js',
			],
			loadStyles: [
				'/static/css/visualize.css',
				'/static/css/visualize-dark.css'
			]
		});
    </script>
</head>
<body>

    <div id="placeholder" style="width:600px;height:300px;"></div>

<script type="text/javascript">
$(function () {
    var d1 = [
        {% for i in range(24) %}
            [ {{i}}, {{ data.hourly[i]}}],
        {% endfor %}
    ];

    $.plot($("#placeholder"), [ d1 ]);
});
</script>

<table style="display:none">
	<caption>Rollup Data</caption>
	<thead>
       		<tr>
			<td></td>
            {% for i in range(24) %}
			<th scope="col">{{ i }}</th>
            {% endfor %}
		</tr>
	</thead>
	<tbody>
		<tr>
            <th scope="row">Hourly</th>
            {% for i in data.hourly %}
            <td>{{ i }}</td>
            {% endfor %}
		</tr>
	</tbody>
</table>

</body>
</html>
